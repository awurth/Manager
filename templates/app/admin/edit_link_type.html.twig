{% extends 'app/admin/layout.html.twig' %}

{% block page_content %}
  {{ form_start(form, { attr: { class: 'mt-4' } }) }}
    {{ form_row(form.name) }}
    {{ form_row(form.color, { attr: { class: 'mb-2' } }) }}
    {{ form_row(form.uriPrefix) }}
    {{ form_row(form.iconFile) }}
    {{ form_rest(form) }}
    <button type="submit" class="btn btn-primary mt-4">Enregistrer</button>
  {{ form_end(form) }}
{% endblock %}
